<% @groups.each do |group| %>
  <% if group.user_ids.include?(current_user.id) %>
    <div>
      <%= link_to "/groups/#{group.id}" do %>
        <%= group.name %>(<%= group.user_ids.count %>)
      <% end %>
      <%= link_to "退会する", group_path(group.id),method: :delete, remote: true %>
    </div>
  <% end %>
<% end %>