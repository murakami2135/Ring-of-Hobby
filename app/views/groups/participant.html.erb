<div class="contaner">
  <div class="row">
    <div class="col-sm-5 text-right mt-5">
      <h1>参加一覧</h1>
    </div>
  </div>
  <div class="row justify-content-center">
    <div class="col-sm-4">
      <% if @group_participant == [] %>
        <%= @group_lists_none %>
      <% else %>
        <div id="group">
          <%= render 'groups/participant', groups: @groups %>
        </div>
      <% end %>
    </div>
  </div>
</div>